{
	"name": "@tuplo/fletcher",
	"description": "Web scraping HTTP request library",
	"version": "0.0.0-development",
	"repository": "git@github.com:tuplo/fletcher.git",
	"author": "Rui Costa",
	"license": "MIT",
	"keywords": [
		"http-client",
		"request",
		"scraping"
	],
	"files": [
		"dist/fletcher.d.ts",
		"dist/helpers/async-retry.d.ts",
		"dist/helpers/cookie-jar.d.ts",
		"dist/index.cjs",
		"dist/index.d.ts",
		"dist/index.mjs",
		"dist/data_files/*"
	],
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"exports": {
		".": [
			{
				"import": {
					"types": "./dist/index.d.ts",
					"default": "./dist/index.mjs"
				},
				"require": {
					"types": "./dist/index.d.ts",
					"default": "./dist/index.cjs"
				},
				"default": "./dist/index.mjs"
			},
			"./dist/index.mjs"
		]
	},
	"types": "dist/index.d.ts",
	"engines": {
		"node": ">=14"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "tsx sh/build.ts",
		"coverage": "tsx sh/coverage.ts",
		"dev": "tsx sh/dev.ts",
		"format": "prettier --write src",
		"lint:ts": "tsc --noEmit",
		"lint": "eslint sh/ src/",
		"test:ci": "vitest run",
		"test": "vitest --watch",
		"upgrade": "npm-check-updates -u -x eslint && npm install"
	},
	"dependencies": {
		"@types/cheerio": "0.22.35",
		"@types/cookie": "0.6.0",
		"@types/retry": "0.12.5",
		"cheerio": "1.0.0-rc.12",
		"cookie": "0.6.0",
		"deepmerge": "4.3.1",
		"got-scraping": "4.0.6",
		"puppeteer-core": "22.13.1",
		"retry": "0.13.1"
	},
	"devDependencies": {
		"@tuplo/shell": "1.2.2",
		"@types/mime": "3.0.4",
		"@types/node": "20.14.11",
		"@vitest/coverage-v8": "2.0.3",
		"esbuild": "0.23.0",
		"eslint": "8.57.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-perfectionist": "2.11.0",
		"eslint-plugin-unicorn": "54.0.0",
		"eslint-plugin-vitest": "0.5.4",
		"mime": "4.0.4",
		"npm-check-updates": "16.14.20",
		"nyc": "17.0.0",
		"prettier": "3.3.3",
		"tsx": "4.16.2",
		"typescript": "5.5.3",
		"typescript-eslint": "7.16.1",
		"vitest": "2.0.3"
	}
}
